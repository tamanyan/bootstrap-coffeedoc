<!doctype html>

<!-- Documentation generated by [CoffeeDoc](http://github.com/omarkhan/coffeedoc) -->

<html>
<head>
  <title>CoffeeDoc</title>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" media="all" href="resources/base.css" />
  <link rel="stylesheet" media="all" href="resources/bootstrap.css" />
</head>
<body>
  <div class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <a href="#" class="navbar-brand">CoffeeDoc &mdash; Module Index</a>
        </div>
      </div>
  </div>
  <div style="margin-top:80px" class="container">
    <div class="row">
      <div class="col-md-4">
        <div class="sidebar">
          <div class="list-group">
            <% for module in @modules: %>
            <a class="list-group-item" href="<%= module.path %>.html"><%= module.path %></a>
            <% end %>
          </div>
        </div>
      </div>
      <div class="col-md-8">
        <div class="content column">
          <% for module in @modules: %>
          <div class="module">
            <div class="header">
              <a href="<%= module.path %>.html"><h1><%= module.basename %></h1></a>
            </div>
            <div class="module-content">
              <% if module.docstring: %><%- module.docstring %><% end %>
              <% if module.classes.length > 0: %>
              <h3>Classes</h3>
              <ul class="classlist">
                <% for cls in module.classes: %>
                <li><a href="<%= module.path %>.html#<%= cls.name %>"><%= cls.name %></a></li>
                <% end %>
              </ul>
              <% end %>
              <% if module.functions.length > 0 or (not @options.hideprivate and module.privateFunctions.length > 0): %>
              <h3>Functions</h3>
              <ul class="functionlist">
                <% for func in module.functions: %>
                <li><a href="<%= module.path %>.html#<%= func.name %>"><%= func.name %>(<%= func.params.join(', ') %>)</a></li>
                <% end %>
                <% if not @options.hideprivate: %>
                  <% for func in module.privateFunctions: %>
                  <li><a href="<%= module.path %>.html#<%= func.name %>"><%= func.name %>(<%= func.params.join(', ') %>)</a></li>
                  <% end %>
                <% end %>
              </ul>
              <% end %>
            </div>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
